
datasource db {
  provider = "sqlite"
}
generator client {
  provider     = "prisma-client-js"
}



model User {
  id        Int   @id @default(autoincrement())
  email     String   @unique
  name      String
  password  String
  role      String   @default("USER")
  isBlocked Boolean  @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt


}
